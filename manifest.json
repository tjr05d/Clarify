{
    "manifest_version": 2,
    "name": "Clarity",
    "version": "0.1", 
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": [
          "content.js"
        ]
      }
    ],
    "background": {
      "scripts": [
        "background.js"
      ]
    },
    "browser_action": {
      "default_icon": "nerd.png"
    },
    "web_accessible_resources": [
      "loading.gif"
    ]
}